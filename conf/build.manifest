		},
		{
			"name": "k3s",
			"repo": "https://github.com/truenas/k3s",
			"branch": "truenas/master"
		},
		{
			"name": "truenas_files",
			"repo": "https://github.com/freenas/freenas",
			"branch": "master",
			"subdir": "src/freenas"
		},
		{
			"name": "middlewared",
			"repo": "https://github.com/freenas/freenas",
			"branch": "master",
			"subdir": "src/middlewared",
			"dependencies": [
				"scst",
				"migrate113",
				"py-libzfs",
				"zettarepl",
				"pkg-pybonjour",
				"pydevd",
				"python-netsnmpagent",
				"aiorwlock",
				"glustercli-python",
				"python-asyncssh",
				"licenselib",
				"migrate93",
				"truenas-files",
				"minio",
				"intel-pcm",
				"plugins",
				"k3s",
				"zectl"
			]
		},
		{
			"name": "truenas",
			"repo": "https://github.com/freenas/freenas",
			"branch": "master",
			"subdir": "debian",
			"dependencies": [
				"middlewared",
				"truenas-webui"
			]
		},
		{
			"name": "truecommand_stats",
			"repo": "https://github.com/freenas/truecommand-stats",
			"branch": "master"
		}
	]
